{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./frontend/js", "./backend", "readme.md", "main.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|doc)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "destination": "./documentation/jsdoc/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/minami"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
        "default": {
            "outputSourceFiles": false,
            "includeDate": false
        }
    }
}
